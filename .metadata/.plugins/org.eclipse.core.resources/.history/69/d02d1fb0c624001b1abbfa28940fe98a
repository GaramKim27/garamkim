package ch02;

public class _12_WhileEx {
	public static void main(String[] args) {
		
		//1.초기값
		int dan = 2;
		int i = 1;
		int a = 1;
		System.out.println("*** 2단 ***");
		//2.조건식
		while(i < 10) {//i가 증가하므로..
		//3.증감식
			a = dan*i;
			System.out.println(dan + "*" + i  + "=" + a);
			i++;
		}
	}
}
