package ch01;

public class _07_InOutEx {
	public static void main(String[] args) {


		// 두수의 합 구하기
		int x = 0; //첫번째숫자를 입력받아 저장
		int y = 0; //두번째숫자를 입력받아 저장
		
		
	    Scanner scan = new Scanner(system.in);
		System.out.println("첫번째 숫자를 입력하세요 : ");
		
		int sum = x + y;
		System.out.println("sum:"+ sum);
		
		
	}

}
