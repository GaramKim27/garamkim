package ch03;

public class _02_MemberMain {
	
	/*
	* String 'birthdate' String name; int age; String address;
	*/

	// 생성자

	// 멈버메서드
	// 멤버변수	
	private String birthDate; //private(캡슐화) : 나한테 직접값을 입력하지마.철벽!!
	private String name;
	private int age;
	private String address;
	
	// setter (값을설정)(멈베변수 대문자로)  
	public void setBirthDate(String birthDate) { //매개체역할(매개변수)
		this.birthDate = birthDate;
	}
	// getter (값을가져오고)
	

	public void showInfo() {
		System.out.println("생년월일 : " + BirthDate);
		System.out.println("이름 : " + Name);
		System.out.println("나이 : " + Age);
		System.out.println("주소 : " + Address);
		

	}

}
