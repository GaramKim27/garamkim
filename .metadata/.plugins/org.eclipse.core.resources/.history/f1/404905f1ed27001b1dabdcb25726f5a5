package ch04;

public class _01_CalMain {
	public static void main(String[] args) {
	
		double num1 = 10.0; 
		double num2 = 5.0;
		
		_01_CalculatorEx cal = new _01_CalculatorEx();
		double addR = cal.add(num1, num2);
		System.out.println("두수의 합 : " + addR);
		
		double subR = cal.add(num1, num2);
		System.out.println("두수의 뺄셈 : " + subR);
		
		double mulR = cal.add(num1, num2);
		System.out.println("두수의 곱 : " + mulR);
		
		double divR = cal.add(num1, num2);
		System.out.println("두수의 나눔 : " + divR);
		
		
		
	
	}
}
