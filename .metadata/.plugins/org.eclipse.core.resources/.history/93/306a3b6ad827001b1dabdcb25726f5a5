package ch03;

public class _02_MemberInfo {
	public static void main(String[] args) {
		// static : 인스턴스 생성하지않고 바로 메서드 사용가능
		// main() 함수는 자바가상머신(JVW)이 프로그램을 시작하기 위해 호출하는 함수
		// 클래스 내부에 만들지만, 클래스의 메소드는 아니다.
		_02_MemberMain MemberInfo = new _02_MemberMain();
		
		MemberInfo.birthDay = "1994년 2월 27일";
		MemberInfo.name = "김가람";
		MemberInfo.age = 27;
		MemberInfo.address = "인천";
		
		MemberInfo.showInfo();
	}
}
		
		// 멤버변수
			/*
			* String 'birthdate' String name; int age; String address;
			 */

				// 생성자

				// 멈버메서드
		
		
		

	
		
	

	
		
	
