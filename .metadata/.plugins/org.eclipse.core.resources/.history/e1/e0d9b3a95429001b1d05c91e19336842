package ch05;

public class _01_Array {
	public static void main(String[] args) {
		
		// 배열자료형[] 배열형 = new 배열자료향[배얄갯수];
		// 배열명[index] = 값;
		
		int[] scores = new int [5];
		scores[0] = 10;
		scores[1] = 20;
		scores[2] = 30;
		scores[3] = 40;
		scores[4] = 50;
		
		System.out.println(scores);
		
	}

}
