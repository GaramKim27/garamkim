package test;

import java.util.Scanner;

public class _04_Kukudan_While {
	 public static void main(String[] args) {
	
	int dan = 0;
	int i = 1;
  	int s = 1;
  	Scanner w = new Scanner(System.in);
  	System.out.print("점수를 입력하세요.");
  	dan = w.nextInt();
  	
	System.out.print("*** " + dan + "단 ***");
	
	while(i<10);{
	s = dan*i;
	System.out.print(dan + "*" + i + " = " + s);
	i++;
	}
	System.out.print("==================");
	w.close();

    }
}