		drink[1] = "소주";
		drink[2] = "맥주";
		drink[3] = "소맥";
		drink[4] = "막걸리";
		drink[5] = "양주";
		
		for(String = 1; String < drink.length drink++) {
			System.out.println(date[drink]);
		}
	}
}
