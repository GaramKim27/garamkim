package ch02;

public class _19_ForEx {
	public static void main(String[] args) {
		/*
		 * 반복문 - 조건이 만족하는 동안 실행
		 * 
		 *  for문의 기본구조
		 *  
		 *  for(초기값; 조건식; 증감식;) {
		 * 
		 * 		수행문장1;
		 * 		수행문장2;
		 * }
		 * 
		 */	
		
		//3단 입력받아 구구단 출력
		int dan = 213;
		System.out.println("=== " + dan + "단 ===");
		for(int a = 1, b = 0; a < 10; a++) {
			b = dan*a;
			System.out.println(dan + "*" + a + ":" + b);
		}
		
	}
}
