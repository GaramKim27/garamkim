package ch05;

public class _02_ArrayEx {
	public static void main(String[] args) {
		
		String[] drink = new String[5];
		
		drink[0] = "소주";
		drink[1] = "맥주";
		drink[2] = "소맥";
		drink[3] = "막걸리";
		drink[4] = "양주";
		
		for(i = 0; i < drink.length; i++) {
			System.out.println(drink[i]);
		}
	}
}
