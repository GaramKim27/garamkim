package ch05;

public class _02_ArrayEx {
	public static void main(String[] args) {
		
		String[] drink = new String[5];
		
		drink[0] = "소주";
		drink[1] = "맥주";
		drink[2] = "소맥";
		drink[3] = "막걸리";
		drink[4] = "양주";
		
		for(int i = 0; i < drink.length; i++) {
			System.out.println(drink[i]);
		}
		String[] drink2 = new String[] {"물", "보리차", "핫식스", "둥글레차", "고냑"};
		
		for(int a = 0; a < drink2.length; a++) {
			System.out.println(drink2[a]);
		}
		
	}
}
